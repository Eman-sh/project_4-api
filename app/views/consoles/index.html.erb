<p id="notice"><%= notice %></p>

<h1>Consoles</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Img1</th>
      <th>Img2</th>
      <th>Img3</th>
      <th>Img4</th>
      <th>Video1</th>
      <th>Video2</th>
      <th>Platform</th>
      <th>Emu</th>
      <th>System</th>
      <th>Price</th>
      <th>Buy</th>
      <th>About</th>
      <th>Info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consoles.each do |console| %>
      <tr>
        <td><%= console.title %></td>
        <td><%= console.img1 %></td>
        <td><%= console.img2 %></td>
        <td><%= console.img3 %></td>
        <td><%= console.img4 %></td>
        <td><%= console.video1 %></td>
        <td><%= console.video2 %></td>
        <td><%= console.platform %></td>
        <td><%= console.emu %></td>
        <td><%= console.system %></td>
        <td><%= console.price %></td>
        <td><%= console.buy %></td>
        <td><%= console.about %></td>
        <td><%= console.info %></td>
        <td><%= link_to 'Show', console %></td>
        <td><%= link_to 'Edit', edit_console_path(console) %></td>
        <td><%= link_to 'Destroy', console, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Console', new_console_path %>
